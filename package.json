{
  "name": "@kaine/vue3-boilerplate",
  "version": "1.1.1",
  "private": false,
  "description": "Quick boilerplate for your next project, containing - Vite, Vuex, Vue Router & Tailwind CSS. This was created as part of my tutorial series.",
  "author": "kainecom <hello@kaine.com> (https://github.com/kainecom/)",
  "scripts": {
    "build": "vite build  --config='vite.config.prod.ts'",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.ts --fix --ignore-path .gitignore",
    "dev": "vite --config='vite.config.dev.ts'",
    "format": "prettier --write src/",
    "jest": "vue-cli-service test:unit",
    "preview": "vite preview --config='./vite.config.prod.ts'",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
